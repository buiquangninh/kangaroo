        <column xsi:type="varchar" name="title" length="1024" nullable="true" comment="Title"/>
    </table>

    <table name="magenest_photoreview_photo" resource="default" engine="innodb" comment="magenest_photoreview_photo">
        <column name="type" xsi:type="text"  comment="File type" />
    </table>

</schema>
