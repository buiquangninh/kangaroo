<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="sales_order_columns">
        <column name="momo_trans_id">
            <settings>
                <filter>text</filter>
                <label translate="true">MoMo Transaction ID</label>
                <visible>true</visible>
            </settings>
        </column>
        <column name="momo_status" component="Magento_Ui/js/grid/columns/select">
            <settings>
                <options class="Magenest\MomoPay\Model\Config\Source\MomoStatus"/>
                <filter>select</filter>
                <dataType>select</dataType>
                <label translate="true">MoMo Status</label>
                <visible>true</visible>
            </settings>
        </column>
    </columns>
</listing>
