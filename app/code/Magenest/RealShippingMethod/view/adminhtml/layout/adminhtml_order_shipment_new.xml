<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="extra_shipment_info">
            <block class="Magento\Shipping\Block\Adminhtml\Order\Tracking" name="real.shipping.method" template="Magenest_RealShippingMethod::order/shipping.phtml">
                <arguments>
                    <argument name="online_carriers" xsi:type="object">Magenest\RealShippingMethod\ViewModel\Carriers</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
