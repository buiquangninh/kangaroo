<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mega_menu>
            <general>
                <enable>0</enable>
                <effect>btt</effect>
            </general>
            <default_conf>
                <txt_color/>
                <hov_txt_color/>
                <btn_bg_color/>
                <hov_btn_bg_color>#25bce9</hov_btn_bg_color>
                <drd_bg_color/>
                <drd_bg_opacity/>
            </default_conf>
            <backup>
                <enable>0</enable>
                <backup_times>7</backup_times>
            </backup>
        </mega_menu>
    </default>
</config>
