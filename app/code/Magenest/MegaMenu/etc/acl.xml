<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magenest_MegaMenu::menu" title="Mega Menu" sortOrder="100">
                    <resource id="Magenest_MegaMenu::manage" title="Manage Menu" sortOrder="10"/>
                    <resource id="Magenest_MegaMenu::label" title="Manage Label" sortOrder="10"/>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_MegaMenu::configuration" title="Mega Menu"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>