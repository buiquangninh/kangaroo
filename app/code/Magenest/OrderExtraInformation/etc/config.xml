<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <oei>
            <vat_invoice>
                <enable>1</enable>
            </vat_invoice>
            <delivery_date>
                <enable>0</enable>
            </delivery_date>
            <delivery_time>
                <enable>1</enable>
            </delivery_time>
            <customer_note>
                <enable>1</enable>
                <show_on>cart,cart_item,checkout</show_on>
            </customer_note>
            <customer_consign>
                <enable>1</enable>
            </customer_consign>
        </oei>
    </default>
</config>
