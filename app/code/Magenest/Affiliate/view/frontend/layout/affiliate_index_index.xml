<?xml version="1.0"?>
<!--

-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\Affiliate\Block\Dashboard" name="mp.affiliate.account.dashboard" template="Magenest_Affiliate::dashboard.phtml" cacheable="false" ifconfig="affiliate/general/enabled"/>
        </referenceContainer>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Sales policy</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="page.main.title.mobile">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">Discount Program</argument>
            </action>
        </referenceBlock>
        <referenceBlock name="page_description" remove="true" />
    </body>
</page>
