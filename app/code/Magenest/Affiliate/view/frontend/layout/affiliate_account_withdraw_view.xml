<?xml version="1.0"?>
<!--

-->
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <body>
        <referenceContainer name="content">
            <referenceBlock name="page.main.title">
                <action method="setPageTitle">
                    <argument translate="true" name="title" xsi:type="string">Kcoin wallet transaction details</argument>
                </action>
            </referenceBlock>
            <referenceBlock name="page.main.title.mobile">
                <action method="setPageTitle">
                    <argument translate="true" name="title" xsi:type="string">Kcoin wallet transaction details</argument>
                </action>
            </referenceBlock>
            <block class="Magenest\Affiliate\Block\Account\Withdraw\View" name="affiliate.account.withdraw.view" template="Magenest_Affiliate::account/withdraw/view.phtml" cacheable="false"/>
        </referenceContainer>
        <referenceBlock name="page_description" remove="true" />
    </body>
</page>
