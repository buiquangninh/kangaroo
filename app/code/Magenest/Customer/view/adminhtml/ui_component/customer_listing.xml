<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <columns name="customer_columns" class="Magento\Customer\Ui\Component\Listing\Columns">
        <column name="street" class="Magenest\Customer\Ui\Component\Listing\Column\DefaultAddress">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">280</item>
                    <item name="filter" xsi:type="string">text</item>
                    <item name="dataType" xsi:type="string">text</item>
                    <item name="label" translate="true" xsi:type="string">Street</item>
                </item>
            </argument>
        </column>
        <column name="ward" class="Magenest\Customer\Ui\Component\Listing\Column\DefaultAddress">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">280</item>
                    <item name="filter" xsi:type="string">false</item>
                    <item name="label" translate="true" xsi:type="string">Ward</item>
                </item>
            </argument>
        </column>
        <column name="district" class="Magenest\Customer\Ui\Component\Listing\Column\DefaultAddress">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">280</item>
                    <item name="filter" xsi:type="string">false</item>
                    <item name="label" translate="true" xsi:type="string">District</item>
                </item>
            </argument>
        </column>
        <column name="city_id" class="Magenest\Customer\Ui\Component\Listing\Column\DefaultAddress">
            <argument name="data" xsi:type="array">
                <item name="options" xsi:type="object">Magenest\Directory\Model\ResourceModel\City\Collection</item>
                <item name="config" xsi:type="array">
                    <item name="sortOrder" xsi:type="number">280</item>
                    <item name="filter" xsi:type="string">select</item>
                    <item name="dataType" xsi:type="string">select</item>
                    <item name="label" translate="true" xsi:type="string">City</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>
